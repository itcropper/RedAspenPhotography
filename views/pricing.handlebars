<style>
    .pricing {border-bottom:1px solid #771013;}
</style>
<section id="pricing" class="type-three" ng-controller="pricingController">
    
    <div class="pricing-top clearfix">
        
        
        <h1 class="text-border">PRICING</h1>
        <div class="row">
            {{#each pricings}}
            <div class="col-md-4">
                <div class="item">
                    <a href="{{this.target}}" class="block">
                        <div class="load-image" alt="Portrait Pricing" data-src="{{this.imgsrc}}"></div>

                        <h3>{{this.title}}</h3>

                    </a>
                </div>
            </div>
            {{/each}}
        </div> 


    </div>

</section>
